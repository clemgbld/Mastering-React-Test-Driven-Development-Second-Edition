    "babel-loader": "^8.2.3",
    "babel-plugin-relay": "^12.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.3",
    "expect-redux": "^5.0.3",
